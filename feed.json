    "title":"{{ post.title }}",
    "subtitle":"{% if post.description %}{{ post.description }}{% else %}{{ post.content | strip_html | strip_newlines | truncate: 120 }}{% endif %}",
    "url":"{{ post.url }}",
    "date":"{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}"
  },{% endfor %}
  []
]